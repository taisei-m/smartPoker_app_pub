<?xml version='1.0' encoding='utf-8'?>
<widget id="com.eajy.smartpoker" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="DecryptResource">
        <param name="android-package" value="com.tkyaji.cordova.DecryptResource" />
        <param name="onload" value="true" />
    </feature>
    <name>Smart Poker</name>
    <description>
      Vue.js + Onsen UI + Cordova sample app
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
      Vue.js + Onsen UI + Cordova teams
    </author>
    <content src="/+++/start-up.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon density="ldpi" src="res/icon/android/android/drawable-hdpi/icon_googleplay.png" />
    <icon density="mdpi" src="res/icon/android/android/drawable-ldpi/icon_googleplay.png" />
    <icon density="hdpi" src="res/icon/android/android/drawable-mdpi/icon_googleplay.png" />
    <icon density="xhdpi" src="res/icon/android/android/drawable-tvdpi/icon_googleplay.png" />
    <icon density="xxhdpi" src="res/icon/android/android/drawable-xhdpi/icon_googleplay.png" />
    <icon density="xxxhdpi" src="res/icon/android/android/drawable-xxhdpi/icon_googleplay.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
